    var mimeType = file.type;
    
    if (fieldVal != undefined || fieldVal != "") {
        var arr = fieldVal.split('\\');
        fileName = arr[arr.length - 1];
        fileExtension = fileName.substr((fileName.lastIndexOf('.') + 1));
    }
