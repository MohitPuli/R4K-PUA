            }else{
                $(existingDetailsSelector).val($(dropDownSelector).val());
            }
            
            console.log('******display ' + $(editButtonSelector).css('display') + '****');
            
            if((currentEditDisplay == 'none' && !$(dropDownSelector).val().startsWith('Other -')) || changeType == 'dropdown')
